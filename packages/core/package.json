{
  "name": "@402kit/core",
  "version": "0.1.0",
  "description": "Core types, schemas, and utilities for 402Kit",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "http",
    "402",
    "payment",
    "x402"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
