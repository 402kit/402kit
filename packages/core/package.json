{
  "name": "@402kit/core",
  "version": "0.1.3",
  "description": "Core SDK for HTTP 402 (x402) payments and entitlements",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "pnpm build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "402kit",
    "http 402",
    "x402",
    "payments",
    "micropayments",
    "sdk",
    "types"
  ],
  "author": "402Kit Authors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/402kit/402kit#readme",
  "bugs": {
    "url": "https://github.com/402kit/402kit/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/402kit/402kit.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
