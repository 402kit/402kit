{
  "name": "@402kit/client",
  "version": "0.1.2",
  "description": "Client utilities to handle HTTP 402/x402 retries (X-PAYMENT)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "402Kit Authors",
  "homepage": "https://github.com/402kit/402kit#readme",
  "bugs": {
    "url": "https://github.com/402kit/402kit/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/402kit/402kit.git",
    "directory": "packages/client"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepack": "pnpm build"
  },
  "keywords": [
    "402kit",
    "http 402",
    "x402",
    "client",
    "fetch",
    "retry",
    "browser",
    "node"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@402kit/core": "^0.1.2"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
