{
  "name": "@402kit/server",
  "version": "0.1.1",
  "description": "Server middleware for 402Kit (Express, Next.js, Hono)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js",
      "require": "./dist/express.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs"
    },
    "./hono": {
      "types": "./dist/hono.d.ts",
      "import": "./dist/hono.js",
      "require": "./dist/hono.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/402kit/402kit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepack": "pnpm build"
  },
  "keywords": [
    "http",
    "402",
    "payment",
    "server",
    "middleware"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@402kit/core": "^0.1.1",
    "@402kit/entitlement": "^0.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "hono": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "hono": {
      "optional": true
    }
  }
}
